<xml id="toolbox" style="display: none">
    <!--Logic-->
    <category name="%{BKY_CATLOGIC}" colour="%{BKY_LOGIC_HUE}">
        <block type="controls_if"></block>
        <block type="logic_compare"></block>
        <block type="logic_operation"></block>
        <block type="logic_negate"></block>
        <block type="logic_boolean"></block>
        <block type="logic_null"></block>
        <block type="logic_ternary"></block>
    </category>
    <!--Loops-->
    <category name="%{BKY_CATLOOPS}" colour="%{BKY_LOOPS_HUE}">
        <block type="controls_repeat_ext">
            <value name="TIMES">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
        </block>
        <block type="controls_whileUntil"></block>
        <block type="controls_for">
            <value name="FROM">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="TO">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
            <value name="BY">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="controls_forEach"></block>
        <block type="controls_flow_statements"></block>
    </category>
    <!--Variable-->
    <category name="%{BKY_CATVARIABLES}" colour="%{BKY_VARIABLES_HUE}" custom="VARIABLE"></category>
    <!--Function-->
    <category name="%{BKY_CATFUNCTIONS}" colour="%{BKY_PROCEDURES_HUE}" custom="PROCEDURE"></category>
    <sep></sep>
    <!--Duckiebots-1-->
    <category id="DuckieBot1" name="%{BKY_CATDUCKIEBOT1}" colour="%{BKY_DUCKIEBOT_HUE}">
        <block type="wait"></block>
        <block type="move">
            <value name="FORWARD_SPEED">
                <block type="math_number">
                    <field name="NUM">40</field>
                </block>
            </value>
            <value name="TURN_SPEED">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
            <value name="DURATION">
                <block type="math_number">
                    <field name="NUM">2</field>
                </block>
            </value>
            <data>
              {
                "requires" : [],
                "provides" : [
                  "commands"
                ]
              }
            </data>
        </block>

        <block type="forward">
            <value name="DURATION">
                <block type="math_number">
                    <field name="NUM">2</field>
                </block>
            </value>
            <data>
              {
                "requires" : [],
                "provides" : [
                  "commands"
                ]
              }
            </data>
        </block>

        <block type="histogram_perception">
            <value name="Bottom Color">
                <block type="colour_picker">
                    <field name="Color">(255,0,0)</field>
                </block>
            </value>
            <value name="TOLERANCE">
                <block type="math_number">
                    <field name="NUM">50</field>
                </block>
            </value>
            <data>
              {
                "requires" : ["supercamera"],
                "provides" : []
              }
            </data>
        </block>
    </category>
    <!--Duckiebots-2-->
    <category id="DuckieBot2" name="%{BKY_CATDUCKIEBOT2}" colour="%{BKY_DUCKIEBOT_HUE}">
    </category>
    <sep></sep>
    <!--Duckiedrone-->
    <category id="DuckieSky" name="%{BKY_CATDUCKIESKY}" colour="%{BKY_DUCKIESKY_HUE}">
    </category>
    <sep></sep>
    <!--DuckieTown-->
    <category id="DuckieTown" name="%{BKY_CATDUCKIETOWN}" colour="%{BKY_DUCKIETOWN_HUE}">
    </category>
</xml>
